{
   "background": {
      "persistent": true,
      "scripts": [ "scripts/background.js", "scripts/backgroundScript.js" ]
   },
   "browser_action": {
      "default_icon": "img/128.png"
   },
   "content_scripts": [ {
      "js": [ "scripts/content.js", "scripts/checkStoped.js" ],
      "matches": [ "https://*/*", "http://*/*" ],
      "run_at": "document_start"
   } ],
   "default_locale": "en",
   "description": "__MSG_description__",
   "icons": {
      "128": "img/128.png",
      "16": "img/16.png",
      "32": "img/32.png",
      "48": "img/48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "__MSG_name__",
   "permissions": [ "webRequest", "webRequestBlocking" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.9",
   "web_accessible_resources": [ "scripts/*" ]
}
